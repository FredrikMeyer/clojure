{
    "gitbook": ">=2.0.0",
    "title": "Clojure, Practicalli",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "plugins": [
        "toggle-chapters",
        "codeblock-disable-glossary",
        "richquotes",
        "disqus",
        "exercises",
        "quizzes",
        "ga",
        "toolbar",
        "sectionx"
    ],
    "pluginsConfig": {
        "richquotes": {
            "todos": true
        },
        "disqus": {
            "shortName": "clojure-practicalli"
        },
        "ga": {
            "token": "UA-28030709-7"
        },
        "toolbar": {
            "buttons":
            [
                {
                    "label": "Gitter Chat",
                    "icon": "fa fa-comments-o",
                    "url": "https://gitter.im/practicalli/chat?utm_source=badge&utm_medium=badge&utm_campaign=pr-badge"
                },
                {
                    "label": "GitHub",
                    "icon": "fa fa-github",
                    "url": "https://github.com/practicalli"
                },
                {
                    "label": "Search page title on StackOverflow",
                    "icon": "fa fa-stack-overflow",
                    "url": "http://stackoverflow.com/search?q={{title}}"
                }
            ]
        }
    }
}
